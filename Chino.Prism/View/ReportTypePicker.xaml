<?xml version="1.0" encoding="UTF-8" ?>
<ContentView
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Chino.Prism.View.ReportTypePicker">
    <ContentView.Content>

        <Picker
            SelectedItem="{Binding Label}">
          <Picker.ItemsSource>
            <x:Array Type="{x:Type x:String}">
              <x:String>Unknown</x:String>
              <x:String>ConfirmedTest</x:String>
              <x:String>ConfirmedClinicalDiagnosis</x:String>
              <x:String>SelfReport</x:String>
              <x:String>Recursive</x:String>
              <x:String>Revoked</x:String>
            </x:Array>
          </Picker.ItemsSource>
        </Picker>

    </ContentView.Content>
</ContentView>
